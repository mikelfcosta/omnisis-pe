{
  "name": "iot",
  "version": "0.4.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "moment": "^2.18.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-alpha.4",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2"
  },
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production TS_NODE_COMPILER_OPTIONS={\\\"target\\\":\\\"es2017\\\"} ts-node -F ./server/index.ts",
    "dev:server": "cross-env NODE_ENV=development TS_NODE_COMPILER_OPTIONS={\\\"target\\\":\\\"es2017\\\"} ts-node -F ./server/index.ts",
    "dev:client": "webpack-dev-server",
    "build": "webpack",
    "test": "jest --runInBand --forceExit"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^21.1.4",
    "@types/node": "^8.0.34",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-redux": "^5.0.10",
    "@types/react-router-dom": "^4.2.2",
    "@types/reactstrap": "^5.0.3",
    "@types/webpack": "^3.0.13",
    "@types/webpack-env": "^1.13.1",
    "@types/webpack-hot-middleware": "^2.16.0",
    "awesome-typescript-loader": "^3.2.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.1.3",
    "ts-node": "^3.3.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.5.3",
    "typings-for-css-modules-loader": "^1.7.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "helpers.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
